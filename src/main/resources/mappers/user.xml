 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.CoinFiesta.CoinFiesta.domain.numberRepository">
	<insert id="save" parameterType="com.CoinFiesta.CoinFiesta.domain.numberRepository">
		insert into
			number
		values(
			0,
			#{number}
		)
	</insert>
</mapper>
        
        